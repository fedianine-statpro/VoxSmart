{
  "AppSettings": {
    // Url to the RSS feed to be processed
    "FeedUrl": "https://feeds.a.dj.com/rss/RSSMarketsMain.xml",
    // Filtering to find publicly traded company names and other securities
    "NLPFilteringEntities": [ "Organization" ],
    // Filtering to find currency codes
    "FilteringRegex": "\\b[A-Z]{3}\\b"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Warning"
    }
  }
}